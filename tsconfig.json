{
  "compilerOptions": {
    "target": "ES2022",
    "lib": [
      "ES2022",
      "DOM",
      "DOM.Iterable"
    ],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "jsx": "preserve",
    "noEmit": true,
    "types": [
      "node"
    ],
    "baseUrl": ".",
    "paths": { "@/*": ["*"] }
  },
  "include": [
    "app",
    "lib",
    "next-env.d.ts"
  ]
}
